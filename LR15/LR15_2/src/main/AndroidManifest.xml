<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.psmsdb.lr15_2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.psmsdb.lr15_2.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.psmsdb.lr15_2.BadStudentsActivity" />
        <activity android:name="com.psmsdb.lr15_2.BestStudentsActivity" />
        <activity android:name="com.psmsdb.lr15_2.FacultyComparisonActivity" />
        <activity android:name="com.psmsdb.lr15_2.GroupComparison2Activity" />
        <activity android:name="com.psmsdb.lr15_2.GroupComparisonActivity" />
        <provider
            android:authorities="com.psmsdb.providers.GroupsList"
            android:name="com.psmsdb.lr15_2.GroupProvider"
            android:exported="true"
            android:multiprocess="true"/>
        <provider
            android:authorities="com.psmsdb.providers.StudentsList"
            android:name="com.psmsdb.lr15_2.StudentProvider"
            android:exported="true"
            android:multiprocess="true"/>
    </application>

</manifest>